
import Pojos.Test

rule "slight case"
    dialect "java"
    when
        op1 :  Test(score < 8)
    then
        System.out.println("Your case seems to be slight");
end

rule "severe case"
    dialect "java"
    when
        op2 : Test(score > 16)
    then
        System.out.println("Your case seems to severe you should o to the hospital as soon as possible");
end